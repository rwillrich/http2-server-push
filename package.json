{
  "name": "http-server-push",
  "description": "Exploration of HTTP/2 Server Push with ES modules",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "node --expose-http2 ./src/http-2",
    "watch": "nodemon",
    "generate-dependencies": "madge --json ./public > ./src/dependencies.json",
    "start-express": "node ./src/http-2-express",
    "start-http": "node ./src/http"
  },
  "engines": {
    "node": "^8.6.0",
    "npm": "^5.3.0"
  },
  "dependencies": {
    "express": "^4.16.1",
    "mime": "^2.0.3",
    "spdy": "^3.4.7"
  },
  "devDependencies": {
    "madge": "^2.2.0",
    "nodemon": "^1.12.1"
  }
}
